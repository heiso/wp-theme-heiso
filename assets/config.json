{
  "entry": {
    "main": [
      "./scripts/main.js",
      "./styles/main.scss"
    ],
    "customizer": [
      "./scripts/customizer.js"
    ]
  },
  "watch": [
    "./styles/**/*.scss",
    "./scripts/**/*.js",
    "templates/**/*.php",
    "src/**/*.php"
  ],
  "publicPath": "/wp-content/themes/dev-theme",
  "devUrl": "http://localdocker:8080",
  "proxyUrl": "http://localhost:3000",
  "cacheBusting": "[name]_[hash:8]"
}
